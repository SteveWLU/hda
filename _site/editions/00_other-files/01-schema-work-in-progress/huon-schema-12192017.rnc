  element damageSpan {
    empty
    >> sch:pattern [
         id =
           "huon-schema-07032017-damageSpan-spanTo-constraint-assert-12"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:damageSpan"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "@spanTo"
             "\x{a}" ~
             "The @spanTo attribute of "
             sch:name [ ]
             " is required."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id =
           "huon-schema-07032017-damageSpan-spanTo_fr-constraint-assert-13"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:damageSpan"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "@spanTo"
             "L'attribut spanTo est requis."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    tei_att.global.attributes,
    tei_att.damaged.attributes,
    tei_att.typed.attributes,
    tei_att.spanning.attributes,
    empty
  }
tei_delSpan =
  
